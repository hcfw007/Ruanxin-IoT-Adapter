body
  margin: 0
  background-color: rgb(240, 242, 245)
  --default-link-color: rgb(63, 144, 247)

*
  box-sizing: border-box

a, a:visited
  text-decoration: none
  color: var(--default-link-color)

.block-white
  background-color: #fff

.block-round
  border-radius: 25px

.main-content>.el-row
  padding: 40px

.no-padding
  padding: 0 !important

.seperator
  min-height: 80px
  width: 0
  border: solid 1px #CCC

.clickable-text
  color: var(--default-link-color)
  cursor: pointer


